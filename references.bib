doi = {https://doi.org/10.1016/j.intfin.2019.01.002},
url = {https://www.sciencedirect.com/science/article/pii/S1042443118301550},
author = {Pi-Hsia Hung and Donald Lien},
keywords = {Market quality, Market microstructure, Order submission},
abstract = {We examine the relationships among stock traders’ trading aggressiveness, order execution quality, and stock price movements in the Taiwan Stock Exchange. Our analyses yield the following findings. Institutional investors have a lower level of trading aggressiveness than individuals. In terms of order immediacy, investment trusts and foreign investors tend to pay higher (lower) prices to buy (sell) stocks. Professional institutions spend much shorter time completing trades per order (<6 min), but obtain higher fill rates (about 99%) than other institutions and individuals. In addition, professional institutions tend to make smart decisions, earning profits for buy orders while reducing losses for sell orders.}
}


