author = {Angelo Ranaldo},
keywords = {Limit order book, Limit orders, Microstructure, Order aggressiveness, Probit model},
abstract = {I examine the information content of a limit order book in a purely order-driven market. I analyze how the state of the limit order book affects a trader's strategy. I develop an econometric technique to study order aggressiveness and provide empirical evidence on the recent theoretical models on limit order book markets. My results show that patient traders become more aggressive when the own (opposite) side book is thicker (thinner), the spread wider, and the temporary volatility increases. Also, I find that the buy and the sell sides of the book affect the order submission differently.}
}

@article{HUNG2019231,
